dynatrace:
  environments:
    - name: E2E-ENV
      url: ${TENANT_APP_URL}
      platformToken: ${PLATFORM_TOKEN}
  queries:
    - id: davis-events
      query: |
        fetch events, from: now()-1h
        | filter testId == "${RUN_UUID}"
        | fields testId, timestamp
auth:
  providers:
    guest: {}

